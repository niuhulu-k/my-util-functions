{
  "extends": [ "prettier"],
  "plugins": [ "import", "promise", "prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "import/extensions": "off",
    "react/jsx-props-no-spreading": "off",
    "react/no-array-index-key": "off"
  },

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
